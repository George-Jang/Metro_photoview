<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#00ff0000"
    android:layout_width="400dp"
    android:layout_height="400dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    >
    <ImageView
        android:id="@+id/btn_center"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:src="@drawable/popcenter"
        />
    <ImageView
        android:id="@+id/btn_station_in"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_above="@+id/btn_center"
        android:layout_alignLeft="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup1"
        />
    <ImageView
        android:id="@+id/txt8"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/btn_station_in"
        android:layout_toRightOf="@+id/btn_station_in"
        android:gravity="center"
        android:src="@drawable/popup12"
        />
    <ImageView
        android:id="@+id/txt2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/btn_center"
        android:layout_toLeftOf="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup3"
        />
    <ImageView
        android:id="@+id/btn_depart"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/txt2"
        android:layout_toLeftOf="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup4"
        />
    <ImageView
        android:id="@+id/txt4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@+id/btn_center"
        android:layout_alignLeft="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup6"
        />
    <ImageView
        android:id="@+id/btn_cancel"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@id/txt4"
        android:layout_toRightOf="@id/txt4"
        android:gravity="center"
        android:src="@drawable/popup7"
        />
    <ImageView
        android:id="@+id/txt7"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignTop="@+id/btn_center"
        android:layout_toRightOf="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup10"
        />
    <ImageView
        android:id="@+id/btn_dest"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/txt7"
        android:layout_toRightOf="@+id/btn_center"
        android:gravity="center"
        android:src="@drawable/popup9"
        />
    <ImageView
        android:id="@+id/txt9"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/btn_depart"
        android:layout_toLeftOf="@+id/txt4"
        android:gravity="center"
        android:src="@drawable/popup5"
        />
    <ImageView
        android:id="@+id/txt10"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_above="@+id/txt2"
        android:layout_toLeftOf="@+id/btn_station_in"
        android:gravity="center"
        android:src="@drawable/popup2"
        />
    <ImageView
        android:id="@+id/txt11"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_below="@id/btn_dest"
        android:layout_toRightOf="@+id/btn_cancel"
        android:gravity="center"
        android:src="@drawable/popup8"
        />
    <ImageView
        android:id="@+id/txt12"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_above="@id/txt7"
        android:layout_toRightOf="@+id/txt8"
        android:src="@drawable/popup11"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="110dp"
        android:text="역내정보" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="130dp"
        android:text="역주변정보" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="60dp"
        android:text="시간표" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="260dp"
        android:layout_marginTop="130dp"
        android:text="즐겨찾기" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="220dp"
        android:text="출발지" />
    <TextView
        android:id="@+id/depart"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="220dp"
        android:text="도착지" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="280dp"
        android:text="종료" />
</RelativeLayout>